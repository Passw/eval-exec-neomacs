; Autoload function/macro wrappers should expose oracle-compatible introspection behavior.
(autoload 'vm-auto-fn "vm-auto-file" nil t)
(functionp 'vm-auto-fn)
(macrop 'vm-auto-fn)
(symbol-function 'vm-auto-fn)
(autoload 'vm-auto-macro "vm-auto-file" nil nil 'macro)
(fboundp 'vm-auto-macro)
(functionp 'vm-auto-macro)
(macrop 'vm-auto-macro)
(symbol-function 'vm-auto-macro)
(indirect-function 'vm-auto-macro t)
