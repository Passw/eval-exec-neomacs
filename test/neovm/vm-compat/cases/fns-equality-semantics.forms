; fns.c â€” equality & association operations

; equal
(equal '(1 2) '(1 2))
(equal '(1 2) '(1 3))
(equal "abc" "abc")
(equal "abc" "ABC")
(equal [1 2] [1 2])
(equal [1 2] [1 3])
(equal nil nil)
(equal 1 1)
(equal 1 1.0)
(equal t nil)
(equal '(1 (2 3)) '(1 (2 3)))

; eql
(eql 1 1)
(eql 1 1.0)
(eql 'a 'a)
(eql 'a 'b)
(eql nil nil)
(eql 1.5 1.5)

; equal-including-properties
(equal-including-properties "abc" "abc")
(equal-including-properties "" "")
(equal-including-properties nil nil)

; memql
(memql 1.0 '(1.0 2.0 3.0))
(memql 1 '(1.0 2.0))
(memql 1 '(1 2 3))
(memql 'a '(b a c))
(memql 99 '(1 2 3))

; rassoc
(rassoc 2 '((a . 1) (b . 2) (c . 3)))
(rassoc 9 '((a . 1)))
(not (null (rassoc nil (list (cons 'a 1) (cons 'b nil)))))
(rassoc 1 nil)
(condition-case err (rassoc 1 5) (error (car err)))

; rassq
(rassq 2 '((a . 1) (b . 2)))
(rassq 2 '((a . 1)))
(not (null (rassq nil (list (list 'a) (cons 'b nil)))))
(rassq 1 nil)

; copy-alist
(copy-alist '((a . 1) (b . 2)))
(copy-alist nil)
(let ((orig '((a . 1) (b . 2)))) (let ((cp (copy-alist orig))) (eq (car orig) (car cp))))
(copy-alist '((a . 1) b (c . 3)))
