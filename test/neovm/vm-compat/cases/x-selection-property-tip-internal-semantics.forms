(mapcar (lambda (sym)
          (list sym
                (fboundp sym)
                (subrp (symbol-function sym))
                (subr-arity (symbol-function sym))
                (commandp sym)))
        '(x-apply-session-resources
          x-change-window-property
          x-delete-window-property
          x-clipboard-yank
          x-disown-selection-internal
          x-get-local-selection
          x-get-selection-internal
          x-own-selection-internal
          x-show-tip))

(list
 (list 'x-apply-session-resources
       (condition-case err (x-apply-session-resources) (error err))
       (condition-case err (x-apply-session-resources nil) (error (car err))))
 (list 'x-change-window-property
       (condition-case err (x-change-window-property "P" "V") (error err))
       (condition-case err (x-change-window-property "P" "V" nil) (error err))
       (condition-case err (x-change-window-property "P" "V" nil nil nil nil nil) (error err))
       (condition-case err (x-change-window-property "P" "V" nil nil nil nil nil nil) (error (car err))))
 (list 'x-delete-window-property
       (condition-case err (x-delete-window-property "P") (error err))
       (condition-case err (x-delete-window-property "P" nil) (error err))
       (condition-case err (x-delete-window-property "P" nil nil) (error err))
       (condition-case err (x-delete-window-property "P" nil nil nil) (error (car err))))
 (list 'x-clipboard-yank
       (condition-case err (x-clipboard-yank) (error err))
       (condition-case err (x-clipboard-yank nil) (error (car err))))
 (list 'x-disown-selection-internal
       (condition-case err (x-disown-selection-internal nil) (error err))
       (condition-case err (x-disown-selection-internal nil nil) (error err))
       (condition-case err (x-disown-selection-internal nil nil nil) (error err))
       (condition-case err (x-disown-selection-internal nil nil nil nil) (error (car err))))
 (list 'x-get-local-selection
       (condition-case err (x-get-local-selection) (error err))
       (condition-case err (x-get-local-selection nil) (error err))
       (condition-case err (x-get-local-selection nil nil) (error err))
       (condition-case err (x-get-local-selection nil nil nil) (error err))
       (condition-case err (x-get-local-selection nil nil nil nil) (error (car err))))
 (list 'x-get-selection-internal
       (condition-case err (x-get-selection-internal nil nil) (error err))
       (condition-case err (x-get-selection-internal nil nil nil) (error err))
       (condition-case err (x-get-selection-internal nil nil nil nil) (error err))
       (condition-case err (x-get-selection-internal nil nil nil nil nil) (error (car err))))
 (list 'x-own-selection-internal
       (condition-case err (x-own-selection-internal nil nil) (error err))
       (condition-case err (x-own-selection-internal nil nil nil) (error err))
       (condition-case err (x-own-selection-internal nil nil nil nil) (error (car err))))
 (list 'x-show-tip
       (condition-case err (x-show-tip "m") (error err))
       (condition-case err (x-show-tip 1) (error err))
       (condition-case err (x-show-tip "m" nil nil nil nil nil) (error err))
       (condition-case err (x-show-tip "m" nil nil nil nil nil nil) (error (car err)))))
