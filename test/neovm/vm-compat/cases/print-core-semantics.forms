; print.c â€” printing functions
; prin1-to-string
(equal (prin1-to-string 42) "42")
(equal (prin1-to-string "hello") "\"hello\"")
(equal (prin1-to-string 'foo) "foo")
(equal (prin1-to-string '(1 2 3)) "(1 2 3)")
(equal (prin1-to-string nil) "nil")
(equal (prin1-to-string t) "t")
(equal (prin1-to-string '(a . b)) "(a . b)")
(equal (prin1-to-string [1 2 3]) "[1 2 3]")
(equal (prin1-to-string ?a) "97")
(equal (prin1-to-string 3.14) "3.14")

; error-message-string
(stringp (error-message-string '(error "test message")))
(equal (error-message-string '(error "hello")) "hello")

; prin1 / princ / print (return their argument)
(= (prin1 42) 42)
(= (princ 42) 42)

; terpri returns t
(eq (terpri) t)

; write-char returns the character
(= (write-char ?a) ?a)
