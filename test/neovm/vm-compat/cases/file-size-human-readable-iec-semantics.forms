; file-size-human-readable-iec formatting and type-error contract.
(file-size-human-readable-iec 0)
(file-size-human-readable-iec 1024)
(file-size-human-readable-iec 1536)
(file-size-human-readable-iec 1023.9)
(file-size-human-readable-iec -1536.5)
(condition-case err
    (file-size-human-readable-iec "x")
  (error (list (car err) (cadr err) (equal (nth 2 err) "x"))))
