; minibuf.c â€” minibuffer operations
; minibuffer-depth
(integerp (minibuffer-depth))
(= (minibuffer-depth) 0)

; minibuffer-prompt
(eq (minibuffer-prompt) nil)

; minibuffer-prompt-end
(integerp (minibuffer-prompt-end))

; minibufferp
(not (minibufferp (current-buffer)))

; minibuffer-window
(windowp (minibuffer-window))

; active-minibuffer-window
(eq (active-minibuffer-window) nil)

; try-completion
(equal (try-completion "he" '("hello" "help" "world")) "hel")
(equal (try-completion "hello" '("hello" "help")) "hello")
(eq (try-completion "xyz" '("hello" "help")) nil)

; all-completions
(equal (all-completions "he" '("hello" "help" "world")) '("hello" "help"))
(equal (all-completions "xyz" '("hello")) nil)

; test-completion
(test-completion "hello" '("hello" "help"))
(not (test-completion "xyz" '("hello")))
