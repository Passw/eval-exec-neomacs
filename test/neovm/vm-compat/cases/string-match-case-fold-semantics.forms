(fboundp 'string-match)
(fboundp 'string-match-p)
(string-match "a" "A")
(string-match-p "a" "A")
(string-match "A" "a")
(string-match-p "A" "a")
(string-match "x" "aéx")
(string-match-p "x" "aéx")
(string-match "x" "aéx" 2)
(string-match-p "x" "aéx" 2)
(string-match "x" "aéx" -1)
(string-match-p "x" "aéx" -1)
(string-match "x" "aéx" 3)
(string-match-p "x" "aéx" 3)
(string-match "\\(é\\)" "aéx")
(string-match-p "\\(é\\)" "aéx")
