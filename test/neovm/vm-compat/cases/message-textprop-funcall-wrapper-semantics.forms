(fboundp 'message-box)
(fboundp 'message-or-box)
(fboundp 'get-pos-property)
(fboundp 'next-char-property-change)
(fboundp 'funcall-interactively)
(fboundp 'funcall-with-delayed-message)
(func-arity 'message-box)
(func-arity 'message-or-box)
(func-arity 'get-pos-property)
(func-arity 'next-char-property-change)
(func-arity 'funcall-interactively)
(func-arity 'funcall-with-delayed-message)
(subr-arity (symbol-function 'message-box))
(subr-arity (symbol-function 'message-or-box))
(subr-arity (symbol-function 'get-pos-property))
(subr-arity (symbol-function 'next-char-property-change))
(subr-arity (symbol-function 'funcall-interactively))
(subr-arity (symbol-function 'funcall-with-delayed-message))
(condition-case err (message-box) (error err))
(condition-case err (message-or-box) (error err))
(condition-case err (get-pos-property) (error err))
(condition-case err (get-pos-property 1) (error err))
(condition-case err (get-pos-property 1 'face nil nil) (error err))
(condition-case err (next-char-property-change) (error err))
(condition-case err (next-char-property-change 1 nil nil) (error err))
(condition-case err (funcall-interactively) (error err))
(condition-case err (funcall-with-delayed-message) (error err))
(condition-case err (funcall-with-delayed-message 0 "x") (error err))
(condition-case err (funcall-with-delayed-message 0 "x" 'ignore nil) (error err))
(message-box "hello %s" "x")
(message-or-box "hello %s" "y")
(funcall-interactively 'car '(1 2))
(funcall-interactively 'ignore)
(funcall-interactively (lambda (x) x) 9)
(funcall-with-delayed-message 0 "x" (lambda () 42))
(funcall-with-delayed-message 0 "x" 'ignore)
(condition-case err (funcall-with-delayed-message 'x "x" 'ignore) (error err))
(condition-case err (funcall-with-delayed-message 0 1 'ignore) (error err))
(condition-case err (funcall-with-delayed-message 0 "x" 1) (error err))
(with-temp-buffer
  (insert "abc")
  (put-text-property 2 3 'vm-prop 'v)
  (list (get-pos-property 2 'vm-prop)
        (get-char-property 2 'vm-prop)))
(with-temp-buffer
  (insert "abc")
  (put-text-property 2 3 'vm-prop 'v)
  (list (next-char-property-change 1)
        (next-property-change 1)))
(with-temp-buffer
  (insert "abc")
  (put-text-property 2 3 'vm-prop 'v)
  (list (next-char-property-change 1 2)
        (next-property-change 1 nil 2)))
