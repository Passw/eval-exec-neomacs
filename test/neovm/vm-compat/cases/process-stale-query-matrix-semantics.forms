(mapcar (lambda (s)
          (list s
                (fboundp s)
                (subrp (symbol-function s))
                (subr-arity (symbol-function s))
                (commandp s)))
        '(process-status
          process-exit-status
          process-name
          process-buffer
          process-command
          process-filter
          process-sentinel
          process-plist
          process-mark
          process-type
          process-thread
          process-coding-system
          process-tty-name
          process-contact
          process-query-on-exit-flag
          set-process-query-on-exit-flag
          process-id
          get-process))

(let ((p (start-process "neovm-proc-stale-query-matrix" nil "cat")))
  (unwind-protect
      (progn
        (delete-process p)
        (list
         (processp p)
         (process-live-p p)
         (eq (process-status p) 'signal)
         (= (process-exit-status p) 9)
         (equal (process-name p) "neovm-proc-stale-query-matrix")
         (null (process-buffer p))
         (equal (process-command p) '("cat"))
         (eq (process-filter p) 'internal-default-process-filter)
         (eq (process-sentinel p) 'internal-default-process-sentinel)
         (null (process-plist p))
         (let ((m (process-mark p)))
           (and (markerp m)
                (null (marker-buffer m))
                (null (marker-position m))))
         (eq (process-type p) 'real)
         (not (processp (process-thread p)))
         (equal (process-coding-system p) '(utf-8-unix . utf-8-unix))
         (stringp (process-tty-name p))
         (process-contact p)
         (process-query-on-exit-flag p)
         (set-process-query-on-exit-flag p t)
         (process-query-on-exit-flag p)
         (integerp (process-id p))
         (get-process "neovm-proc-stale-query-matrix")))
    (ignore-errors (delete-process p))))
