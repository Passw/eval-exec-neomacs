; x-open/x-close should keep strict type predicates across non-string designators.
(condition-case err (x-open-connection 'foo) (error err))
(condition-case err (x-close-connection 'foo) (error err))

(condition-case err (x-open-connection [foo]) (error err))
(condition-case err (x-close-connection [foo]) (error err))

(condition-case err (x-open-connection '(foo)) (error err))
(condition-case err (x-close-connection '(foo)) (error err))

(condition-case err (x-open-connection '(foo . bar)) (error err))
(condition-case err (x-close-connection '(foo . bar)) (error err))
