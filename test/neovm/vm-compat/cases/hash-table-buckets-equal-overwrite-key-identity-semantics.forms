(let* ((k1 (copy-sequence "x"))
       (k2 (copy-sequence "x"))
       (ht (make-hash-table :test 'equal :size 5)))
  (puthash k1 'a ht)
  (puthash k2 'b ht)
  (aset k2 0 ?y)
  (let* ((entries (apply #'append (internal--hash-table-buckets ht)))
         (key (caar entries)))
    (list (hash-table-count ht)
          (gethash "x" ht 'miss)
          (gethash "y" ht 'miss)
          key
          (equal key "x")
          (eq key k1)
          (eq key k2))))
