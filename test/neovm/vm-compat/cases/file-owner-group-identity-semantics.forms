; file ownership identity builtins: return shape, runtime consistency, arity.
(fboundp 'file-user-uid)
(fboundp 'file-group-gid)
(integerp (file-user-uid))
(integerp (file-group-gid))
(>= (file-user-uid) 0)
(>= (file-group-gid) 0)

(list (= (file-user-uid) (user-real-uid))
      (= (file-group-gid) (group-real-gid)))

(condition-case err (file-user-uid nil) (error (car err)))
(condition-case err (file-group-gid nil) (error (car err)))
