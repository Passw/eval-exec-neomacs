; casetab.c — case table operations (oracle-safe subset)
; case-table-p — basic type checks
(case-table-p nil)
(case-table-p 42)
(case-table-p "hello")

; current-case-table returns something case-table-p recognizes
(case-table-p (current-case-table))
(case-table-p (standard-case-table))

; set-case-table roundtrip
(let ((orig (current-case-table)))
  (set-case-table orig)
  (case-table-p (current-case-table)))

; set-standard-case-table roundtrip
(let ((orig (standard-case-table)))
  (set-standard-case-table orig)
  (case-table-p (standard-case-table)))

; error: wrong type argument
(condition-case err (set-case-table 42) (error (car err)))
(condition-case err (set-case-table nil) (error (car err)))
(condition-case err (set-standard-case-table 42) (error (car err)))
