; input mode helper setters should match batch-mode side effects and arity.
(set-input-mode t nil t 7)
(current-input-mode)
(list (set-input-interrupt-mode nil) (current-input-mode))
(list (set-input-interrupt-mode t) (current-input-mode))
(condition-case err (set-input-interrupt-mode nil nil) (list (car err) (cdr err)))
(list (set-input-meta-mode nil) (current-input-mode))
(list (set-input-meta-mode t) (current-input-mode))
(condition-case err (set-input-meta-mode) (list (car err) (cdr err)))
(list (set-output-flow-control nil) (current-input-mode))
(list (set-output-flow-control t) (current-input-mode))
(condition-case err (set-output-flow-control nil nil) (list (car err) (cdr err)))
(list (set-quit-char 7) (current-input-mode))
(list (set-quit-char 65) (current-input-mode))
(condition-case err (set-quit-char) (list (car err) (cdr err)))
(condition-case err (set-quit-char 7 8) (list (car err) (cdr err)))
