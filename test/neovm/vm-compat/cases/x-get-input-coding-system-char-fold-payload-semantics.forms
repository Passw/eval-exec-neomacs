;; Lock-in: `x-get-input-coding-system` downcases character-code payloads
;; in `(wrong-type-argument stringp <arg>)` signals.
(let ((probe (lambda (arg)
               (condition-case err
                   (list 'ok (x-get-input-coding-system arg))
                 (error (list (car err) (nth 1 err) (nth 2 err)))))))
  (list
   (list 'int-65 (funcall probe 65))
   (list 'char-A (funcall probe ?A))
   (list 'int-90 (funcall probe 90))
   (list 'char-Z (funcall probe ?Z))
   (list 'int-97 (funcall probe 97))
   (list 'char-a (funcall probe ?a))
   (list 'int-49 (funcall probe 49))
   (list 'char-1 (funcall probe ?1))
   (list 'dot-i-uppercase (funcall probe 304))
   (list 'modifier-capital-i (funcall probe 7305))
   (list 'kelvin-sign (funcall probe 8490))
   (list 'adlam-uppercase-o (funcall probe 42955))
   (list 'adlam-uppercase-u (funcall probe 42956))
   (list 'adlam-uppercase-ka (funcall probe 42958))
   (list 'adlam-uppercase-laam (funcall probe 42962))
   (list 'adlam-uppercase-miim (funcall probe 42964))
   (list 'adlam-uppercase-nya (funcall probe 42970))
   (list 'adlam-uppercase-haa (funcall probe 42972))
   (list 'supp-preserve-range-a-start (funcall probe 68944))
   (list 'supp-preserve-range-a-end (funcall probe 68965))
   (list 'supp-preserve-range-b-start (funcall probe 93856))
   (list 'supp-preserve-range-b-end (funcall probe 93880))
   (list 'supp-fold-control (funcall probe 66560))
   (list 'negative (funcall probe -1))
   (list 'too-large (funcall probe 1114112))))
