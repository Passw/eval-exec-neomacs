(condition-case err (other-window) (error (car err)))
(condition-case err (other-window nil) (error err))
(condition-case err (other-window "x") (error err))
(condition-case err (other-window (make-marker)) (error err))
(condition-case err (other-window 1 nil nil nil) (error (car err)))
(let* ((w1 (progn (delete-other-windows) (selected-window)))
       (w2 (split-window)))
  (list
    (progn (other-window 1 nil nil) (eq (selected-window) w2))
    (progn (select-window w1) (other-window 1.5 nil nil) (eq (selected-window) w2))
    (progn (select-window w1) (other-window 0.4 nil nil) (eq (selected-window) w1))
    (progn (select-window w1) (other-window -0.4 nil nil) (eq (selected-window) w2))
    (progn (select-window w1) (other-window -1.2 nil nil) (eq (selected-window) w1))))
