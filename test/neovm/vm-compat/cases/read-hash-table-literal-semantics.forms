; #s(hash-table ...) should read as a real hash table object, not a placeholder list.
(let ((h (car (read-from-string "#s(hash-table size 3 test equal data (\"a\" 1 \"b\" 2))"))))
  (list (hash-table-p h)
        (hash-table-test h)
        (hash-table-count h)
        (gethash "a" h 'miss)
        (gethash "b" h 'miss)))

(let* ((h (car (read-from-string "#s(hash-table size 3 test equal data (\"a\" 1 \"b\" 2))")))
       (k (copy-sequence "a")))
  (gethash k h 'miss))

(let ((h (car (read-from-string "#s(hash-table size 1 test equal data (\"a\" 1 \"a\" 2))"))))
  (list (hash-table-count h)
        (gethash "a" h 'miss)))

(with-temp-buffer
  (insert "#s(hash-table size 3 test equal data (\"a\" 1 \"b\" 2))")
  (goto-char (point-min))
  (let ((h (read (current-buffer))))
    (list (hash-table-p h)
          (hash-table-test h)
          (hash-table-count h)
          (gethash "b" h 'miss))))
