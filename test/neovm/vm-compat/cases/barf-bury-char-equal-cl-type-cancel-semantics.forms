(fboundp 'barf-if-buffer-read-only)
(subrp (symbol-function 'barf-if-buffer-read-only))
(subr-arity (symbol-function 'barf-if-buffer-read-only))

(fboundp 'bury-buffer-internal)
(subrp (symbol-function 'bury-buffer-internal))
(subr-arity (symbol-function 'bury-buffer-internal))

(fboundp 'char-equal)
(subrp (symbol-function 'char-equal))
(subr-arity (symbol-function 'char-equal))

(fboundp 'cl-type-of)
(subrp (symbol-function 'cl-type-of))
(subr-arity (symbol-function 'cl-type-of))

(fboundp 'cancel-kbd-macro-events)
(subrp (symbol-function 'cancel-kbd-macro-events))
(subr-arity (symbol-function 'cancel-kbd-macro-events))

(list 'char-equal-default (char-equal ?a ?A))
(let ((case-fold-search nil))
  (list 'char-equal-case-sensitive (char-equal ?a ?A)))
(let ((case-fold-search t))
  (list 'char-equal-case-fold (char-equal ?a ?A)))
(condition-case err
    (char-equal 1 "a")
  (wrong-type-argument (list 'char-equal-type (car err) (cdr err))))

(list 'cl-type-of
      (cl-type-of nil)
      (cl-type-of t)
      (cl-type-of 1)
      (cl-type-of 1.5)
      (cl-type-of "x")
      (cl-type-of 'foo)
      (cl-type-of '(1 . 2))
      (cl-type-of [1 2])
      (cl-type-of (make-hash-table))
      (cl-type-of (symbol-function 'car))
      (cl-type-of (lambda (x) x)))

(with-temp-buffer
  (bury-buffer-internal (current-buffer)))
(condition-case err
    (bury-buffer-internal 'x)
  (wrong-type-argument (list 'bury-buffer-internal-type (car err) (cdr err))))
(condition-case err
    (bury-buffer-internal)
  (wrong-number-of-arguments (car err)))

(with-temp-buffer
  (setq buffer-read-only nil)
  (barf-if-buffer-read-only))
(with-temp-buffer
  (setq buffer-read-only t)
  (condition-case err
      (barf-if-buffer-read-only)
    (buffer-read-only (car err))))
(with-temp-buffer
  (setq buffer-read-only t)
  (condition-case err
      (barf-if-buffer-read-only 0)
    (args-out-of-range (list 'barf-if-buffer-read-only-range (car err) (cdr err)))))
(with-temp-buffer
  (setq buffer-read-only t)
  (condition-case err
      (barf-if-buffer-read-only "x")
    (wrong-type-argument (list 'barf-if-buffer-read-only-type (car err) (cdr err)))))
(condition-case err
    (barf-if-buffer-read-only nil nil)
  (wrong-number-of-arguments (car err)))

(cancel-kbd-macro-events)
(let ((unread-command-events '(97 98)))
  (list (cancel-kbd-macro-events) unread-command-events))
(condition-case err
    (cancel-kbd-macro-events nil)
  (wrong-number-of-arguments (car err)))
