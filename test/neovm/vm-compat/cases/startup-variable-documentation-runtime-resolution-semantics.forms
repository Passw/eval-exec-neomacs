(list
 (let ((n 0))
   (mapatoms
    (lambda (s)
      (let ((d (get s 'variable-documentation)))
        (when (and (integerp d)
                   (stringp (documentation-property s 'variable-documentation t)))
          (setq n (1+ n))))))
   n)
