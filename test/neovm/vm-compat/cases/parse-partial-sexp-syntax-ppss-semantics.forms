; Baseline parser-state semantics for structural context queries.

(fboundp 'parse-partial-sexp)
(fboundp 'syntax-ppss)

(with-temp-buffer
  (insert "abc")
  (parse-partial-sexp 1 4))

(with-temp-buffer
  (insert "(a)")
  (parse-partial-sexp 1 3))

(with-temp-buffer
  (insert "(a)")
  (syntax-ppss 3))

(condition-case err
    (parse-partial-sexp 1)
  (wrong-number-of-arguments (car err)))

(condition-case err
    (syntax-ppss 1 2)
  (wrong-number-of-arguments (car err)))

(condition-case err
    (parse-partial-sexp "1" 2)
  (error (car err)))

(condition-case err
    (syntax-ppss "1")
  (error (car err)))
