; accept-process-output: milliseconds argument is fixnum-only when present,
; and controls whether SECONDS must be fixnum vs generic number.
(condition-case err (accept-process-output nil 0.1 "x") (error err))
(condition-case err (accept-process-output nil nil "x") (error err))
(condition-case err (accept-process-output nil 1 "x") (error err))
(condition-case err (accept-process-output nil nil 0) (error err))
(condition-case err (accept-process-output nil 0.1 nil) (error err))
(condition-case err (accept-process-output nil 0.1 0) (error err))
(condition-case err (accept-process-output nil 0.1 0 t) (error err))
(condition-case err (accept-process-output nil 1 2) (error err))
(condition-case err (accept-process-output nil 1 2 nil) (error err))
