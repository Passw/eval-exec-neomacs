; keyboard.c — keyboard/input operations
; this-command-keys
(stringp (this-command-keys))

; this-command-keys-vector
(vectorp (this-command-keys-vector))

; recent-keys
(vectorp (recent-keys))

; recursion-depth
(integerp (recursion-depth))
(= (recursion-depth) 0)

; input-pending-p
(not (input-pending-p))

; open-dribble-file — nil disables
(eq (open-dribble-file nil) nil)

; current-input-mode returns a list
(listp (current-input-mode))
(= (length (current-input-mode)) 4)

; discard-input returns nil
(eq (discard-input) nil)

; event-convert-list
(integerp (event-convert-list '(control ?a)))

; prefix-numeric-value
(= (prefix-numeric-value nil) 1)
(= (prefix-numeric-value 4) 4)
(= (prefix-numeric-value '-) -1)

; posn-at-point — returns position info
(with-temp-buffer
  (insert "hello")
  (goto-char 1)
  (listp (posn-at-point)))
