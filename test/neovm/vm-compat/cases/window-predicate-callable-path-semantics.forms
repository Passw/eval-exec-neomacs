(mapcar (lambda (s) (list s (fboundp s)))
        '(windowp window-live-p window-valid-p))

(list (subr-arity (symbol-function 'windowp))
      (subr-arity (symbol-function 'window-live-p))
      (subr-arity (symbol-function 'window-valid-p)))

(let ((w (selected-window)))
  (list (funcall #'windowp w)
        (apply #'windowp (list w))
        (let ((fn (indirect-function 'windowp)))
          (funcall fn w))
        (funcall #'window-live-p w)
        (apply #'window-live-p (list w))
        (let ((fn (indirect-function 'window-live-p)))
          (funcall fn w))
        (funcall #'window-valid-p w)
        (apply #'window-valid-p (list w))
        (let ((fn (indirect-function 'window-valid-p)))
          (funcall fn w))))

(let ((w (split-window)))
  (delete-window w)
  (list (funcall #'windowp w)
        (apply #'windowp (list w))
        (let ((fn (indirect-function 'windowp)))
          (funcall fn w))
        (funcall #'window-live-p w)
        (apply #'window-live-p (list w))
        (let ((fn (indirect-function 'window-live-p)))
          (funcall fn w))
        (funcall #'window-valid-p w)
        (apply #'window-valid-p (list w))
        (let ((fn (indirect-function 'window-valid-p)))
          (funcall fn w))))

(list (funcall #'windowp nil)
      (apply #'windowp '(999999))
      (funcall (indirect-function 'windowp) "x")
      (funcall #'window-live-p nil)
      (apply #'window-live-p '(999999))
      (funcall (indirect-function 'window-live-p) 'foo)
      (funcall #'window-valid-p nil)
      (apply #'window-valid-p '(999999))
      (funcall (indirect-function 'window-valid-p) 'foo))

(list (condition-case err (funcall #'windowp) (error (car err)))
      (condition-case err (apply #'windowp '()) (error (car err)))
      (condition-case err (funcall #'windowp nil nil) (error (car err)))
      (condition-case err (apply #'windowp '(nil nil)) (error (car err)))
      (condition-case err (funcall #'window-live-p) (error (car err)))
      (condition-case err (apply #'window-live-p '()) (error (car err)))
      (condition-case err (funcall #'window-live-p nil nil) (error (car err)))
      (condition-case err (apply #'window-live-p '(nil nil)) (error (car err)))
      (condition-case err (funcall #'window-valid-p) (error (car err)))
      (condition-case err (apply #'window-valid-p '()) (error (car err)))
      (condition-case err (funcall #'window-valid-p nil nil) (error (car err)))
      (condition-case err (apply #'window-valid-p '(nil nil)) (error (car err))))
