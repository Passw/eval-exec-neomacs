(condition-case err (x-display-pixel-width 'foo) (error err))
(condition-case err (x-display-pixel-height 'foo) (error err))

(condition-case err (x-display-pixel-width [foo]) (error err))
(condition-case err (x-display-pixel-height [foo]) (error err))

(condition-case err (x-display-pixel-width '(foo)) (error err))
(condition-case err (x-display-pixel-height '(foo)) (error err))

(condition-case err (x-display-pixel-width '(foo . bar)) (error err))
(condition-case err (x-display-pixel-height '(foo . bar)) (error err))
