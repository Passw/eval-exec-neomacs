; ccl symbol registration/runtime designator semantics.
(mapcar (lambda (s)
          (list s
                (fboundp s)
                (subrp (symbol-function s))
                (subr-arity (symbol-function s))
                (commandp s)))
        '(register-ccl-program register-code-conversion-map ccl-execute ccl-execute-on-string))

(register-ccl-program 'neovm-ccl-sym-a [10 0 0])
(register-ccl-program 'neovm-ccl-sym-b [10 0 0])
(register-ccl-program 'neovm-ccl-sym-a [10 0 0 0])

(register-code-conversion-map 'neovm-ccl-map-a [10 0 0])
(register-code-conversion-map 'neovm-ccl-map-b [1 2 3])
(register-code-conversion-map 'neovm-ccl-map-a [4 5 6])

(condition-case err
    (ccl-execute 'neovm-ccl-sym-a [0 0 0 0 0 0 0 0])
  (error err))

(condition-case err
    (ccl-execute-on-string 'neovm-ccl-sym-a [0 0 0 0 0 0 0 0 0] "abc")
  (error err))

(condition-case err
    (ccl-execute 'neovm-ccl-missing [0 0 0 0 0 0 0 0])
  (error err))

(condition-case err
    (ccl-execute-on-string 'neovm-ccl-missing [0 0 0 0 0 0 0 0 0] "abc")
  (error err))
