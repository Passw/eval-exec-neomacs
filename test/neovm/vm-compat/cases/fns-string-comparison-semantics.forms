; fns.c â€” string comparison & search operations

; compare-strings
(compare-strings "abc" 0 3 "abc" 0 3)
(compare-strings "abc" 0 3 "ABC" 0 3)
(compare-strings "abc" 0 3 "ABC" 0 3 t)
(compare-strings "abc" 0 2 "abd" 0 2)
(compare-strings "abc" 1 3 "bc" 0 2)
(compare-strings "abc" 0 3 "ab" 0 2)
(compare-strings "" 0 0 "" 0 0)
(condition-case err (compare-strings 5 0 1 "a" 0 1) (error (car err)))

; string-equal / string=
(string-equal "foo" "foo")
(string-equal "foo" "FOO")
(string-equal "foo" "bar")
(string-equal "" "")
(string-equal "abc" 'abc)
(condition-case err (string-equal 5 "a") (error (car err)))

; string-lessp / string<
(string-lessp "abc" "abd")
(string-lessp "abc" "abc")
(string-lessp "abc" "ab")
(string-lessp "ab" "abc")
(string-lessp "" "a")
(string-lessp "a" "")
(string-lessp "abc" 'abd)
(condition-case err (string-lessp 5 "a") (error (car err)))

; string-search
(string-search "ll" "hello")
(string-search "xx" "hello")
(string-search "lo" "hello" 2)
(string-search "lo" "hello" 4)
(string-search "" "hello")
(string-search "" "hello" 5)
(string-search "" "")
(condition-case err (string-search 5 "a") (error (car err)))
(condition-case err (string-search "a" 5) (error (car err)))
(condition-case err (string-search "a" "abc" -1) (error (car err)))

; string-distance
(string-distance "kitten" "sitting")
(string-distance "abc" "abc")
(string-distance "" "abc")
(string-distance "abc" "")
(string-distance "" "")
(string-distance "abc" "ABC")
(string-distance "abc" "ABC" t)
(condition-case err (string-distance 5 "a") (error (car err)))

; string-version-lessp
(string-version-lessp "foo2" "foo10")
(string-version-lessp "foo10" "foo2")
(string-version-lessp "abc" "abc")
(string-version-lessp "" "a")

; string-as-multibyte / string-as-unibyte
(string-as-multibyte "abc")
(string-as-unibyte "abc")
(condition-case err (string-as-multibyte 5) (error (car err)))

; string-to-multibyte / string-to-unibyte
(string-to-multibyte "abc")
(condition-case err (string-to-multibyte 5) (error (car err)))

; string-make-multibyte / string-make-unibyte
(string-make-multibyte "abc")
(string-make-unibyte "abc")
