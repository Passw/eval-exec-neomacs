(fboundp 'remove-images)

(remove-images 1 10)
(remove-images ?a ?z)

(condition-case err
    (remove-images "x" 10)
  (error err))
(condition-case err
    (remove-images 1 "x")
  (error err))

(condition-case err
    (remove-images 1 10 1)
  (error err))
(condition-case err
    (remove-images 1 10 nil)
  (error err))

(condition-case err
    (remove-images 1)
  (error (car err)))
