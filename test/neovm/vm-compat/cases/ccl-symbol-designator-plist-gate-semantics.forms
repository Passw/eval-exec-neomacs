; CCL symbol designators are gated by the ccl-program-idx plist property.
(put 'neovm-ccl-manual-gate 'ccl-program-idx 1)
(ccl-program-p 'neovm-ccl-manual-gate)
(register-ccl-program 'neovm-ccl-gate [10 0 0])
(setplist 'neovm-ccl-gate nil)
(register-ccl-program 'neovm-ccl-gate [10 0 0 0])
(get 'neovm-ccl-gate 'ccl-program-idx)
(ccl-program-p 'neovm-ccl-gate)
(condition-case err
    (ccl-execute 'neovm-ccl-gate [0 0 0 0 0 0 0 0])
  (error err))
(condition-case err
    (ccl-execute-on-string 'neovm-ccl-gate [0 0 0 0 0 0 0 0 0] "abc")
  (error err))
(setplist 'neovm-ccl-gate 1)
(condition-case err
    (register-ccl-program 'neovm-ccl-gate [10 0 0 0])
  (error err))
(ccl-program-p 'neovm-ccl-gate)
