; make-serial-process :port contract compatibility semantics.
(mapcar (lambda (s)
          (list s
                (fboundp s)
                (subrp (symbol-function s))
                (subr-arity (symbol-function s))
                (commandp s)))
        '(make-serial-process))

(list
 (condition-case err (make-serial-process :name "sp" :port t :speed 9600) (error err))
 (condition-case err (make-serial-process :name "sp" :port 1 :speed 9600) (error err))
 (condition-case err (make-serial-process :name "sp" :port nil :speed 9600) (error err))
 (condition-case err (make-serial-process :name "sp" :speed 9600) (error err))
 (condition-case err (make-serial-process :name "sp" :port "pty") (error err)))
