(fboundp 'category-set-mnemonics)
(subrp (symbol-function 'category-set-mnemonics))
(subr-arity (symbol-function 'category-set-mnemonics))

(fboundp 'check-coding-systems-region)
(subrp (symbol-function 'check-coding-systems-region))
(subr-arity (symbol-function 'check-coding-systems-region))

(fboundp 'command-remapping)
(subrp (symbol-function 'command-remapping))
(subr-arity (symbol-function 'command-remapping))

(fboundp 'command-modes)
(subrp (symbol-function 'command-modes))
(subr-arity (symbol-function 'command-modes))

(fboundp 'close-font)
(subrp (symbol-function 'close-font))
(subr-arity (symbol-function 'close-font))

(category-set-mnemonics (make-category-set "a1!"))
(condition-case err
    (category-set-mnemonics nil)
  (wrong-type-argument (list 'category-set-mnemonics-type (car err) (cdr err))))
(condition-case err
    (category-set-mnemonics [1 2 3])
  (wrong-type-argument
   (list 'category-set-mnemonics-vector-type (car err) (cdr err))))
(condition-case err
    (category-set-mnemonics)
  (wrong-number-of-arguments (car err)))

(check-coding-systems-region 1 1 '(utf-8))
(check-coding-systems-region "x" 1 'utf-8)
(condition-case err
    (check-coding-systems-region 1 "x" 'utf-8)
  (wrong-type-argument
   (list 'check-coding-systems-region-type (car err) (cdr err))))
(condition-case err
    (check-coding-systems-region 1 1)
  (wrong-number-of-arguments (car err)))

(command-modes 'ignore)
(condition-case err
    (command-modes)
  (wrong-number-of-arguments (car err)))

(command-remapping 'ignore)
(command-remapping 'ignore nil)
(command-remapping 'ignore nil nil)
(condition-case err
    (command-remapping 'ignore nil t)
  (wrong-type-argument
   (list 'command-remapping-keymap-type (car err) (cdr err))))
(condition-case err
    (command-remapping)
  (wrong-number-of-arguments (car err)))
(condition-case err
    (command-remapping 'ignore nil nil nil)
  (wrong-number-of-arguments (car err)))

(condition-case err
    (close-font nil)
  (wrong-type-argument (list 'close-font-type (car err) (cdr err))))
(condition-case err
    (close-font (font-spec :family "Mono"))
  (wrong-type-argument (list 'close-font-font-spec-type (car err))))
(condition-case err
    (close-font 1)
  (wrong-type-argument (list 'close-font-int-type (car err) (cdr err))))
(condition-case err
    (close-font)
  (wrong-number-of-arguments (car err)))
(condition-case err
    (close-font nil nil nil)
  (wrong-number-of-arguments (car err)))
