; display-color-p/x-display-color-p should preserve get-device-terminal payloads
; across non-terminal designator shapes.
(condition-case err (display-color-p [foo]) (error err))
(condition-case err (x-display-color-p [foo]) (error err))

(condition-case err (display-color-p '(foo)) (error err))
(condition-case err (x-display-color-p '(foo)) (error err))

(condition-case err (display-color-p '(foo . bar)) (error err))
(condition-case err (x-display-color-p '(foo . bar)) (error err))
