(let ((tm (run-at-time "2 min" nil #'ignore)))
  (prog1 (timerp tm)
    (cancel-timer tm)))

(let ((tm (run-at-time "1.5 hour" nil #'ignore)))
  (prog1 (timerp tm)
    (cancel-timer tm)))

(let ((tm (run-with-timer "1 week" nil #'ignore)))
  (prog1 (timerp tm)
    (cancel-timer tm)))

(let ((tm (run-at-time "2 fortnights" nil #'ignore)))
  (prog1 (timerp tm)
    (cancel-timer tm)))
