; dired.c â€” directory listing
; directory-files
(listp (directory-files "/tmp"))
(> (length (directory-files "/tmp")) 0)
(stringp (car (directory-files "/tmp")))

; directory-files with match
(listp (directory-files "/tmp" nil nil))

; file-attributes
(listp (file-attributes "/tmp"))
(> (length (file-attributes "/tmp")) 5)

; error paths
(condition-case err (directory-files "/nonexistent-xyz-99") (error (car err)))
(condition-case err (directory-files 42) (error (car err)))
