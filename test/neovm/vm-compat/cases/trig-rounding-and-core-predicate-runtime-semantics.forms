; core math + predicate semantics not deeply covered elsewhere.
(fboundp 'acos)
(fboundp 'asin)
(fboundp 'atan)
(fboundp 'cos)
(fboundp 'tan)
(fboundp 'sqrt)
(fboundp 'ceiling)
(fboundp 'floor)

(list (zerop (acos 1.0))
      (> (asin 1.0) 1.5)
      (> (atan 1.0) 0.7)
      (= (cos 0.0) 1.0)
      (zerop (tan 0.0))
      (= (sqrt 4.0) 2.0)
      (= (ceiling 3.2) 4)
      (= (floor 3.8) 3))
(condition-case err (acos 2.0) (error (car err)))
(condition-case err (asin 2.0) (error (car err)))
(condition-case err (sqrt -1.0) (error (car err)))
(condition-case err (atan) (error (car err)))
(condition-case err (tan 0.0 1.0) (error (car err)))

(fboundp 'arrayp)
(fboundp 'atom)
(fboundp 'keywordp)
(fboundp 'make-vector)
(fboundp 'nlistp)
(fboundp 'sequencep)
(fboundp 'string-lessp)

(list (arrayp [1 2])
      (arrayp "ab")
      (arrayp '(1 2)))
(list (atom nil)
      (atom '(1 . 2))
      (atom 'x))
(list (keywordp :ok)
      (keywordp 'ok)
      (keywordp nil))
(equal (make-vector 3 'x) [x x x])
(condition-case err (make-vector -1 nil) (error (car err)))
(list (nlistp '(1 2))
      (nlistp 12)
      (nlistp nil))
(list (sequencep '(1 2))
      (sequencep [1 2])
      (sequencep "ab")
      (sequencep :x))
(list (string-lessp "abc" "abd")
      (string-lessp "abd" "abc")
      (string-lessp "abc" "abc"))
(condition-case err (string-lessp "abc") (error (car err)))
(condition-case err (string-lessp "abc" 12) (error (car err)))

