(fboundp 'backward-prefix-chars)
(with-temp-buffer
  (insert "'`foo")
  (goto-char 3)
  (backward-prefix-chars)
  (point))
(with-temp-buffer
  (modify-syntax-entry ?' ". p")
  (insert "''foo")
  (goto-char 3)
  (backward-prefix-chars)
  (point))
(condition-case err (backward-prefix-chars 1) (wrong-number-of-arguments (car err)))
