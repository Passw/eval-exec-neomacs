; register-ccl-program nil/non-vector type contract semantics.
(mapcar (lambda (s)
          (list s
                (fboundp s)
                (subrp (symbol-function s))
                (subr-arity (symbol-function s))
                (commandp s)))
        '(register-ccl-program ccl-program-p ccl-execute))

(register-ccl-program 'neovm-ccl-nil nil)
(ccl-program-p 'neovm-ccl-nil)

(condition-case err
    (register-ccl-program 'neovm-ccl-type "x")
  (error err))

(condition-case err
    (register-ccl-program 'neovm-ccl-type 1)
  (error err))

(condition-case err
    (register-ccl-program 'neovm-ccl-type 'foo)
  (error err))

(register-ccl-program 'neovm-ccl-nil [10 0 0])
(condition-case err
    (ccl-execute 'neovm-ccl-nil [0 0 0 0 0 0 0 0])
  (error err))
