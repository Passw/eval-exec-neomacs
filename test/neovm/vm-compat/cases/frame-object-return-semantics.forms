; Frame-returning APIs should expose frame objects, not integers.
(let ((f (selected-frame)))
  (list (framep f)
        (frame-live-p f)
        (integerp f)
        (numberp f)
        (listp f)
        (vectorp f)
        (eq f (window-frame))
        (eq f (car (frame-list)))
        (= (length (frame-list)) 1)))
(let* ((f (window-frame (selected-window)))
       (wlist (window-list f)))
  (list (framep f)
        (frame-live-p f)
        (integerp f)
        (listp wlist)
        (= (length wlist) 1)
        (windowp (car wlist))
        (eq f (window-frame (car wlist)))))
(listp (window-list (window-frame)))
(let ((f (selected-frame)))
  (let ((s (select-frame f)))
    (list (framep s)
          (eq s f)
          (eq (selected-frame) f))))
