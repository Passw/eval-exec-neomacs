; Compatibility coverage for backtrace helper wrappers.
(subr-arity (symbol-function 'backtrace--frames-from-thread))
(subr-arity (symbol-function 'backtrace--locals))
(subr-arity (symbol-function 'backtrace-debug))
(subr-arity (symbol-function 'backtrace-eval))
(subr-arity (symbol-function 'backtrace-frame--internal))

(condition-case err (backtrace--frames-from-thread) (error err))
(condition-case err (backtrace--frames-from-thread nil) (error err))
(let ((frames (backtrace--frames-from-thread (current-thread))))
  (list (listp frames) (consp frames) (listp (car frames))))

(condition-case err (backtrace--locals) (error err))
(condition-case err (backtrace--locals nil) (error err))
(condition-case err (backtrace--locals 0) (error err))

(condition-case err (backtrace-debug) (error err))
(condition-case err (backtrace-debug 0) (error err))
(condition-case err (backtrace-debug 0 0 nil nil) (error err))

(condition-case err (backtrace-eval) (error err))
(condition-case err (backtrace-eval 0 nil) (error err))
(condition-case err (backtrace-eval 0 0 nil nil) (error err))

(condition-case err (backtrace-frame--internal) (error err))
(condition-case err (backtrace-frame--internal 0) (error err))
(condition-case err (backtrace-frame--internal 0 0) (error err))
(condition-case err (backtrace-frame--internal 0 0 nil) (error err))
