; font/face batch compatibility: no-op and argument-validation slices
(fboundp 'list-fonts)
(fboundp 'find-font)
(fboundp 'font-family-list)
(fboundp 'clear-font-cache)
(fboundp 'face-id)
(fboundp 'face-font)
(fboundp 'internal-set-font-selection-order)
(fboundp 'internal-set-alternative-font-family-alist)
(fboundp 'internal-set-alternative-font-registry-alist)

(condition-case err
    (list-fonts (font-spec :family "NeovmSans") (selected-frame))
  (error (car err)))
(condition-case err
    (find-font (font-spec :family "NeovmSans") (selected-frame))
  (error (car err)))
(condition-case err
    (font-family-list (selected-frame))
  (error (car err)))
(condition-case err
    (clear-font-cache)
  (error (car err)))
(condition-case err
    (clear-font-cache 1)
  (error (car err)))
(condition-case err
    (face-id 'default 999999)
  (error (car err)))
(condition-case err
    (face-id nil)
  (error (car err)))
(condition-case err
    (face-font 'default nil nil)
  (error (car err)))
(condition-case err
    (face-font 1)
  (error (car err)))
(condition-case err
    (internal-set-font-selection-order '(:width :height :weight :slant))
  (error (car err)))
(condition-case err
    (internal-set-font-selection-order nil)
  (error (car err)))
(condition-case err
    (internal-set-alternative-font-family-alist '(("Sans" "Sans-Serif")))
  (error (car err)))
(condition-case err
    (internal-set-alternative-font-registry-alist '(("iso8859-1" "unicode-bmp")))
  (error (car err)))
