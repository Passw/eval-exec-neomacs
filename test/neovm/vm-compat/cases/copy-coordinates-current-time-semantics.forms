(list
 (list 'copy-keymap (fboundp 'copy-keymap) (func-arity 'copy-keymap) (commandp 'copy-keymap))
 (list 'copy-category-table (fboundp 'copy-category-table) (func-arity 'copy-category-table) (commandp 'copy-category-table))
 (list 'coordinates-in-window-p (fboundp 'coordinates-in-window-p) (func-arity 'coordinates-in-window-p) (commandp 'coordinates-in-window-p))
 (list 'current-cpu-time (fboundp 'current-cpu-time) (func-arity 'current-cpu-time) (commandp 'current-cpu-time))
 (list 'current-idle-time (fboundp 'current-idle-time) (func-arity 'current-idle-time) (commandp 'current-idle-time)))
(let* ((k (make-sparse-keymap))
       (_ (define-key k (kbd "a") 'foo))
       (c (copy-keymap k)))
  (list (keymapp c)
        (eq c k)
        (eq (lookup-key c (kbd "a")) 'foo)))
(let* ((k (make-sparse-keymap))
       (_ (define-key k (kbd "C-x a") 'foo))
       (c (copy-keymap k)))
  (define-key c (kbd "C-x b") 'bar)
  (list (lookup-key k (kbd "C-x b"))
        (lookup-key c (kbd "C-x b"))
        (lookup-key k (kbd "C-x a"))
        (lookup-key c (kbd "C-x a"))))
(condition-case err (copy-keymap nil) (error err))
(let* ((std (standard-category-table))
       (cpy (copy-category-table)))
  (list (category-table-p cpy)
        (eq cpy std)
        (eq (char-table-subtype cpy) 'category-table)))
(let* ((tbl (make-category-table))
       (cpy (copy-category-table tbl)))
  (list (category-table-p cpy)
        (eq cpy tbl)
        (eq (char-table-subtype cpy) 'category-table)))
(condition-case err (copy-category-table 1) (error err))
(coordinates-in-window-p '(1 . 2) nil)
(coordinates-in-window-p '(9999 . 9999) nil)
(condition-case err (coordinates-in-window-p 1 nil) (error err))
(condition-case err (coordinates-in-window-p '(a . 1) nil) (error err))
(condition-case err (coordinates-in-window-p '(1 2) nil) (error err))
(condition-case err (coordinates-in-window-p '(1 . 2) 'foo) (error err))
(condition-case err (coordinates-in-window-p '(1 . 2)) (error err))
(let ((cpu (current-cpu-time)))
  (list (consp cpu)
        (integerp (car cpu))
        (eq (cdr cpu) 1000000)))
(condition-case err (current-cpu-time 1) (error err))
(current-idle-time)
(condition-case err (current-idle-time 1) (error err))
