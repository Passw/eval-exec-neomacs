(list
 (let ((p (make-network-process :name "neo-live-net" :server t :service 0)))
   (unwind-protect
       (list
        (process-live-p p)
        (equal (process-live-p p) '(run open listen connect stop))
        (process-status p)
        (delete-process p)
        (process-live-p p)
        (process-status p))
     (ignore-errors (delete-process p))))
 (let ((p (make-pipe-process :name "neo-live-pipe")))
   (unwind-protect
       (list
        (process-live-p p)
        (equal (process-live-p p) '(run open listen connect stop))
        (process-status p)
        (delete-process p)
        (process-live-p p)
        (process-status p))
     (ignore-errors (delete-process p)))))
