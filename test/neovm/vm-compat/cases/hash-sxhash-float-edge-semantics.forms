(sxhash-eql 0.0)
(sxhash-eql -0.0)
(sxhash-equal 0.0)
(sxhash-equal -0.0)
(eql 0.0 -0.0)
(equal 0.0 -0.0)

(let ((x (/ 0.0 0.0)))
  (list (eql x x) (equal x x) (sxhash-eql x) (sxhash-equal x)))

(let ((ht (make-hash-table :test 'eql))
      (nan (/ 0.0 0.0)))
  (puthash 0.0 'zero ht)
  (puthash nan 'nan ht)
  (list (gethash -0.0 ht 'miss)
        (gethash nan ht 'miss)))

(let ((ht (make-hash-table :test 'equal))
      (nan (/ 0.0 0.0)))
  (puthash 0.0 'zero ht)
  (puthash nan 'nan ht)
  (list (gethash -0.0 ht 'miss)
        (gethash nan ht 'miss)))
