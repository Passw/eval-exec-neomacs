; Compatibility coverage for predicate/file/length/format helpers.
(subr-arity (symbol-function 'number-or-marker-p))
(subr-arity (symbol-function 'obarrayp))
(subr-arity (symbol-function 'vector-or-char-table-p))
(subr-arity (symbol-function 'function-equal))
(subr-arity (symbol-function 'special-variable-p))
(subr-arity (symbol-function 'module-function-p))
(subr-arity (symbol-function 'user-ptrp))
(subr-arity (symbol-function 'symbol-with-pos-p))
(subr-arity (symbol-function 'symbol-with-pos-pos))
(subr-arity (symbol-function 'file-accessible-directory-p))
(subr-arity (symbol-function 'file-executable-p))
(subr-arity (symbol-function 'length<))
(subr-arity (symbol-function 'length=))
(subr-arity (symbol-function 'length>))
(subr-arity (symbol-function 'substring-no-properties))
(subr-arity (symbol-function 'format-message))

(number-or-marker-p 1)
(number-or-marker-p 1.5)
(number-or-marker-p ?a)
(number-or-marker-p nil)
(obarrayp obarray)
(obarrayp [nil])
(vector-or-char-table-p [])
(vector-or-char-table-p (make-char-table nil))
(function-equal 'car 'car)
(function-equal "x" "x")
(special-variable-p 'load-path)
(special-variable-p 'car)
(condition-case err (special-variable-p 1) (error err))
(module-function-p nil)
(user-ptrp nil)
(symbol-with-pos-p 'sym)
(condition-case err (symbol-with-pos-pos 'sym) (error err))

(let ((d (make-temp-file "vm-accdir-" t)))
  (prog1 (file-accessible-directory-p d)
    (delete-directory d)))
(file-accessible-directory-p "/definitely/not/here")
(condition-case err (file-accessible-directory-p 1) (error err))
(let ((f (make-temp-file "vm-exec-")))
  (set-file-modes f #o644)
  (prog1 (file-executable-p f)
    (delete-file f)))
(let ((f (make-temp-file "vm-exec-")))
  (set-file-modes f #o755)
  (prog1 (file-executable-p f)
    (delete-file f)))
(condition-case err (file-executable-p 1) (error err))

(length< '(1 2) 3)
(length< '(1 . 2) 2)
(length= '(1 . 2) 1)
(length> [1 2 3] 2)
(condition-case err (length< 1 2) (error err))
(condition-case err (length< '(1 2) "2") (error err))

(substring-no-properties "abcdef" 1 4)
(condition-case err (substring-no-properties) (error err))

(format-message "a=%s" 1)
(condition-case err (format-message) (error err))
(condition-case err (format-message 1) (error err))
