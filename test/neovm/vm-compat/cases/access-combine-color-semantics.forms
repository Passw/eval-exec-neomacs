(fboundp 'access-file)
(subrp (symbol-function 'access-file))
(subr-arity (symbol-function 'access-file))

(fboundp 'combine-after-change-execute)
(subrp (symbol-function 'combine-after-change-execute))
(subr-arity (symbol-function 'combine-after-change-execute))

(fboundp 'color-values-from-color-spec)
(subrp (symbol-function 'color-values-from-color-spec))
(subr-arity (symbol-function 'color-values-from-color-spec))

(fboundp 'color-gray-p)
(subrp (symbol-function 'color-gray-p))
(subr-arity (symbol-function 'color-gray-p))

(fboundp 'color-supported-p)
(subrp (symbol-function 'color-supported-p))
(subr-arity (symbol-function 'color-supported-p))

(access-file "/tmp" "read")
(condition-case err
    (access-file "/definitely-not-here-neovm" "read")
  (file-missing
   (list 'access-file-missing
         (car err)
         (car (cdr err))
         (car (last (cdr err))))))
(condition-case err
    (access-file 1 "read")
  (wrong-type-argument (list 'access-file-file-type (car err) (cdr err))))
(condition-case err
    (access-file "/tmp" 1)
  (wrong-type-argument (list 'access-file-operation-type (car err) (cdr err))))
(condition-case err
    (access-file "/tmp")
  (wrong-number-of-arguments (car err)))

(combine-after-change-execute)
(condition-case err
    (combine-after-change-execute nil)
  (wrong-number-of-arguments (car err)))

(list 'color-values-from-color-spec
      (color-values-from-color-spec "#000")
      (color-values-from-color-spec "#fff")
      (color-values-from-color-spec "#112233")
      (color-values-from-color-spec "#111122223333"))
(list 'color-values-from-color-spec-invalid
      (color-values-from-color-spec "#abcd")
      (color-values-from-color-spec "bogus"))
(condition-case err
    (color-values-from-color-spec 1)
  (wrong-type-argument
   (list 'color-values-from-color-spec-type (car err) (cdr err))))
(condition-case err
    (color-values-from-color-spec)
  (wrong-number-of-arguments (car err)))

(list 'color-gray-p
      (color-gray-p "#000000")
      (color-gray-p "#808080")
      (color-gray-p "#ff0000")
      (color-gray-p "#fff" nil)
      (color-gray-p "bogus"))
(condition-case err
    (color-gray-p 1)
  (wrong-type-argument (list 'color-gray-p-type (car err) (cdr err))))
(condition-case err
    (color-gray-p "#fff" 0)
  (wrong-type-argument (list 'color-gray-p-frame (car err) (cdr err))))
(condition-case err
    (color-gray-p)
  (wrong-number-of-arguments (car err)))

(list 'color-supported-p
      (color-supported-p "#123456")
      (color-supported-p "#fff" nil t)
      (color-supported-p "bogus" nil nil))
(condition-case err
    (color-supported-p 1)
  (wrong-type-argument (list 'color-supported-p-type (car err) (cdr err))))
(condition-case err
    (color-supported-p "#fff" 1)
  (wrong-type-argument (list 'color-supported-p-frame (car err) (cdr err))))
(condition-case err
    (color-supported-p)
  (wrong-number-of-arguments (car err)))
(condition-case err
    (color-supported-p "#fff" nil t nil)
  (wrong-number-of-arguments (car err)))
