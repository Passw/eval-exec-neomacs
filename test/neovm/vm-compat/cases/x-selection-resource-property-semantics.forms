;; Newly implemented x-* primitive surface: selection/resource/property/font helpers.
(mapcar (lambda (sym)
          (list sym
                (fboundp sym)
                (subrp (symbol-function sym))
                (subr-arity (symbol-function sym))))
        '(x-backspace-delete-keys-p
          x-family-fonts
          x-get-atom-name
          x-get-resource
          x-list-fonts
          x-parse-geometry
          x-selection-exists-p
          x-selection-owner-p
          x-uses-old-gtk-dialog
          x-window-property
          x-window-property-attributes))

(list 'x-backspace-delete-keys-p
      (condition-case err (x-backspace-delete-keys-p) (error err))
      (condition-case err (x-backspace-delete-keys-p nil) (error err))
      (condition-case err (x-backspace-delete-keys-p (selected-frame)) (error err))
      (condition-case err (x-backspace-delete-keys-p 1) (error err))
      (condition-case err (x-backspace-delete-keys-p nil nil) (error (car err))))

(list (list 'x-family-fonts
            (x-family-fonts)
            (x-family-fonts nil nil)
            (condition-case err (x-family-fonts "abc" nil) (error err))
            (condition-case err (x-family-fonts "abc" (selected-frame)) (error err))
            (condition-case err (x-family-fonts "abc" 1) (error err))
            (condition-case err (x-family-fonts 1 nil) (error err))
            (condition-case err (x-family-fonts 1 1) (error err))
            (condition-case err (x-family-fonts nil nil nil) (error (car err))))
      (list 'x-list-fonts
            (condition-case err (x-list-fonts nil) (error err))
            (condition-case err (x-list-fonts nil nil nil nil nil) (error err))
            (condition-case err (x-list-fonts) (error (car err)))
            (condition-case err (x-list-fonts nil nil nil nil nil nil) (error (car err))))
      (list 'x-get-resource
            (condition-case err (x-get-resource nil nil) (error err))
            (condition-case err (x-get-resource nil nil nil nil) (error err))
            (condition-case err (x-get-resource nil) (error (car err)))
            (condition-case err (x-get-resource nil nil nil nil nil) (error (car err)))))

(list (list 'x-selection-exists-p
            (x-selection-exists-p)
            (x-selection-exists-p nil)
            (x-selection-exists-p nil nil)
            (x-selection-exists-p 'PRIMARY)
            (x-selection-exists-p 'PRIMARY 'STRING)
            (x-selection-exists-p 'PRIMARY 1)
            (condition-case err (x-selection-exists-p 1) (error err))
            (condition-case err (x-selection-exists-p 1 'STRING) (error err))
            (condition-case err (x-selection-exists-p nil nil nil) (error (car err))))
      (list 'x-selection-owner-p
            (x-selection-owner-p)
            (x-selection-owner-p nil)
            (x-selection-owner-p nil nil)
            (x-selection-owner-p 'PRIMARY)
            (x-selection-owner-p 'PRIMARY 'STRING)
            (x-selection-owner-p 'PRIMARY 1)
            (condition-case err (x-selection-owner-p 1) (error err))
            (condition-case err (x-selection-owner-p 1 'STRING) (error err))
            (condition-case err (x-selection-owner-p nil nil nil) (error (car err)))))

(list 'x-parse-geometry
      (x-parse-geometry "80x24+10+20")
      (x-parse-geometry "80x24")
      (x-parse-geometry "")
      (x-parse-geometry "x")
      (condition-case err (x-parse-geometry nil) (error err))
      (condition-case err (x-parse-geometry 1) (error err))
      (condition-case err (x-parse-geometry "80x24+10") (error err))
      (condition-case err (x-parse-geometry "80x24+10+20+30") (error err))
      (condition-case err (x-parse-geometry nil nil) (error (car err))))

(list (list 'x-get-atom-name
            (condition-case err (x-get-atom-name 'WM_CLASS) (error err))
            (condition-case err (x-get-atom-name 'WM_CLASS (selected-frame)) (error err))
            (condition-case err (x-get-atom-name 'WM_CLASS 1) (error err))
            (condition-case err (x-get-atom-name) (error (car err)))
            (condition-case err (x-get-atom-name 'WM_CLASS nil nil) (error (car err))))
      (list 'x-window-property
            (condition-case err (x-window-property "WM_NAME") (error err))
            (condition-case err (x-window-property "WM_NAME" nil) (error err))
            (condition-case err (x-window-property "WM_NAME" (selected-frame)) (error err))
            (condition-case err (x-window-property "WM_NAME" 1) (error err))
            (condition-case err (x-window-property "WM_NAME" nil nil nil nil nil) (error err))
            (condition-case err (x-window-property) (error (car err)))
            (condition-case err (x-window-property "WM_NAME" nil nil nil nil nil nil) (error (car err))))
      (list 'x-window-property-attributes
            (condition-case err (x-window-property-attributes "WM_NAME") (error err))
            (condition-case err (x-window-property-attributes "WM_NAME" nil nil) (error err))
            (condition-case err (x-window-property-attributes "WM_NAME" (selected-frame)) (error err))
            (condition-case err (x-window-property-attributes "WM_NAME" 1) (error err))
            (condition-case err (x-window-property-attributes) (error (car err)))
            (condition-case err (x-window-property-attributes "WM_NAME" nil nil nil) (error (car err)))))

(list 'x-uses-old-gtk-dialog
      (x-uses-old-gtk-dialog)
      (condition-case err (x-uses-old-gtk-dialog nil) (error (car err))))
