; kill-ring-yank-pointer duplicate/structural-tail semantics.
(setq kill-ring (list "a" "a" "b"))
(setq kill-ring-yank-pointer (cdr kill-ring))
(current-kill 0 t)

(setq kill-ring (list "a" "a" "b"))
(setq kill-ring-yank-pointer (list "a" "b"))
(current-kill 0 t)
(current-kill 1 nil)
(car kill-ring-yank-pointer)

(setq kill-ring (list "x" "a" "a"))
(setq kill-ring-yank-pointer (list "a" "a"))
(current-kill 0 t)
(current-kill 1 nil)
(car kill-ring-yank-pointer)

(setq kill-ring (list "x" "a" "a"))
(setq kill-ring-yank-pointer (list "q" "a"))
(current-kill 0 nil)
(car kill-ring-yank-pointer)
