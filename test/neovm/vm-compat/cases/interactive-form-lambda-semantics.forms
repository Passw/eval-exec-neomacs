(list
 (interactive-form 'ignore)
 (let ((orig (symbol-function 'vm-if-lambda)))
   (unwind-protect
       (progn
         (fset 'vm-if-lambda '(lambda nil (interactive "p") 1))
         (interactive-form 'vm-if-lambda))
     (fset 'vm-if-lambda orig)))
 (let ((orig (symbol-function 'vm-if-lambda-doc)))
   (unwind-protect
       (progn
         (fset 'vm-if-lambda-doc '(lambda nil "doc" (interactive "P") 1))
         (interactive-form 'vm-if-lambda-doc))
     (fset 'vm-if-lambda-doc orig)))
 (let ((orig-lambda (symbol-function 'vm-if-lambda))
       (orig-alias (symbol-function 'vm-if-alias)))
   (unwind-protect
       (progn
         (fset 'vm-if-lambda '(lambda nil (interactive "r") 1))
         (fset 'vm-if-alias 'vm-if-lambda)
         (interactive-form 'vm-if-alias))
     (fset 'vm-if-lambda orig-lambda)
     (fset 'vm-if-alias orig-alias)))
 (let ((orig (symbol-function 'vm-if-lambda-plain)))
   (unwind-protect
       (progn
         (fset 'vm-if-lambda-plain '(lambda nil 1))
         (interactive-form 'vm-if-lambda-plain))
     (fset 'vm-if-lambda-plain orig)))
 (interactive-form '(lambda nil (interactive "p") 1))
 (interactive-form '(lambda nil "doc" (interactive "P") 1))
 (interactive-form '(lambda nil 1))
 (interactive-form 0)
 (condition-case err (interactive-form) (error err)))
