(mapcar (lambda (s) (list s (fboundp s)))
        '(next-window previous-window))

(list (subr-arity (symbol-function 'next-window))
      (subr-arity (symbol-function 'previous-window)))

(let ((w (selected-window)))
  (list (windowp (funcall #'next-window))
        (windowp (apply #'next-window '()))
        (let ((fn (indirect-function 'next-window)))
          (windowp (funcall fn)))
        (windowp (funcall #'next-window w nil nil))
        (windowp (apply #'next-window (list w t nil)))
        (let ((fn (indirect-function 'next-window)))
          (windowp (funcall fn w nil nil)))))

(let ((w (selected-window)))
  (list (windowp (funcall #'previous-window))
        (windowp (apply #'previous-window '()))
        (let ((fn (indirect-function 'previous-window)))
          (windowp (funcall fn)))
        (windowp (funcall #'previous-window w nil nil))
        (windowp (apply #'previous-window (list w t nil)))
        (let ((fn (indirect-function 'previous-window)))
          (windowp (funcall fn w nil nil)))))

(let ((w2 (split-window)))
  (unwind-protect
      (let ((w1 (selected-window))
            (nextfn (indirect-function 'next-window))
            (prevfn (indirect-function 'previous-window)))
        (list (windowp (funcall #'next-window w1 nil nil))
              (windowp (apply #'next-window (list w1 nil nil)))
              (windowp (funcall nextfn w1 nil nil))
              (windowp (funcall #'previous-window w1 nil nil))
              (windowp (apply #'previous-window (list w1 nil nil)))
              (windowp (funcall prevfn w1 nil nil))))
    (when (window-live-p w2)
      (delete-window w2))))

(list (condition-case err (funcall #'next-window 999999) (error (car err)))
      (condition-case err (apply #'next-window '(999999)) (error (car err)))
      (condition-case err (funcall #'next-window nil nil nil nil) (error (car err)))
      (condition-case err (apply #'next-window '(nil nil nil nil)) (error (car err)))
      (condition-case err (funcall #'previous-window 999999) (error (car err)))
      (condition-case err (apply #'previous-window '(999999)) (error (car err)))
      (condition-case err (funcall #'previous-window nil nil nil nil) (error (car err)))
      (condition-case err (apply #'previous-window '(nil nil nil nil)) (error (car err))))
