; Baseline scan-sexps compatibility.

(fboundp 'scan-sexps)

(with-temp-buffer
  (insert "(a b)")
  (scan-sexps 1 1))

(with-temp-buffer
  (insert "(a b)")
  (scan-sexps 1 -1))

(condition-case err (scan-sexps 1) (wrong-number-of-arguments (car err)))
(condition-case err (scan-sexps "1" 1) (error (car err)))
(condition-case err (scan-sexps 1 "1") (error (car err)))
