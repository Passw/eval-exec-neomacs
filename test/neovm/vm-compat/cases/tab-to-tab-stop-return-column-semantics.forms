; tab-to-tab-stop should return the reached display column.
(list
 (with-temp-buffer
   (list (current-column)
         (tab-to-tab-stop)
         (current-column)
         (buffer-string)))
 (with-temp-buffer
   (insert "abc")
   (goto-char (point-max))
   (list (current-column)
         (tab-to-tab-stop)
         (current-column)
         (buffer-string)))
 (condition-case err (tab-to-tab-stop 1) (error err)))
