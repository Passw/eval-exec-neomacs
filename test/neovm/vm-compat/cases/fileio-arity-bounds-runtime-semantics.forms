; insert-file-contents/write-region should enforce oracle arity bounds.
(let ((f (make-temp-file "ifc-arity-ok-")))
  (unwind-protect
      (with-temp-buffer
        (cadr (insert-file-contents f nil nil nil nil)))
    (ignore-errors (delete-file f))))

(let ((f (make-temp-file "ifc-arity-bad-")))
  (unwind-protect
      (with-temp-buffer
        (condition-case err
            (insert-file-contents f nil nil nil nil nil)
          (error (car err))))
    (ignore-errors (delete-file f))))

(let ((f (make-temp-file "wr-arity-ok-")))
  (unwind-protect
      (with-temp-buffer
        (insert "x")
        (write-region nil nil f nil nil nil nil))
    (ignore-errors (delete-file f))))

(let ((f (make-temp-file "wr-arity-bad-")))
  (unwind-protect
      (with-temp-buffer
        (insert "x")
        (condition-case err
            (write-region nil nil f nil nil nil nil nil)
          (error (car err))))
    (ignore-errors (delete-file f))))
