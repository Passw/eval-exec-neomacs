(fboundp 'accessible-keymaps)
(subrp (symbol-function 'accessible-keymaps))
(subr-arity (symbol-function 'accessible-keymaps))

(fboundp 'coding-system-plist)
(subrp (symbol-function 'coding-system-plist))
(subr-arity (symbol-function 'coding-system-plist))

(fboundp 'add-face-text-property)
(subrp (symbol-function 'add-face-text-property))
(subr-arity (symbol-function 'add-face-text-property))

(fboundp 'bidi-resolved-levels)
(subrp (symbol-function 'bidi-resolved-levels))
(subr-arity (symbol-function 'bidi-resolved-levels))

(fboundp 'bidi-find-overridden-directionality)
(subrp (symbol-function 'bidi-find-overridden-directionality))
(subr-arity (symbol-function 'bidi-find-overridden-directionality))

(fboundp 'color-distance)
(subrp (symbol-function 'color-distance))
(subr-arity (symbol-function 'color-distance))

(let ((m (make-sparse-keymap)))
  (and (consp (accessible-keymaps m))
       (equal (caar (accessible-keymaps m)) [])))
(let ((m (make-sparse-keymap)))
  (keymapp (cdar (accessible-keymaps m))))
(let ((m (make-sparse-keymap)))
  (define-key m (kbd "C-x") (make-sparse-keymap))
  (equal (mapcar #'car (accessible-keymaps m [24])) '([24])))
(let ((m (make-sparse-keymap)))
  (accessible-keymaps m [a]))
(condition-case err
    (accessible-keymaps nil)
  (wrong-type-argument (list 'accessible-keymaps-map (car err) (cdr err))))
(condition-case err
    (accessible-keymaps (make-sparse-keymap) t)
  (wrong-type-argument (list 'accessible-keymaps-prefix-seq (car err) (cdr err))))
(condition-case err
    (accessible-keymaps (make-sparse-keymap) '(a))
  (wrong-type-argument (list 'accessible-keymaps-prefix-array (car err) (cdr err))))

(plist-get (coding-system-plist 'utf-8) :name)
(plist-get (coding-system-plist 'latin-1) :name)
(plist-get (coding-system-plist 'utf-8-unix) :name)
(plist-get (coding-system-plist nil) :name)
(plist-get (coding-system-plist 'utf-8) :mnemonic)
(condition-case err
    (coding-system-plist "x")
  (wrong-type-argument (list 'coding-system-plist-type (car err) (cdr err))))
(condition-case err
    (coding-system-plist 'bogus)
  (coding-system-error (list 'coding-system-plist-unknown (car err) (cdr err))))

(with-temp-buffer
  (insert "abc")
  (add-face-text-property 1 2 'bold)
  (get-text-property 1 'face))
(with-temp-buffer
  (insert "abc")
  (put-text-property 1 2 'face 'italic)
  (add-face-text-property 1 2 'bold t)
  (get-text-property 1 'face))
(with-temp-buffer
  (insert "abc")
  (put-text-property 1 2 'face 'italic)
  (add-face-text-property 1 2 'bold nil)
  (get-text-property 1 'face))
(condition-case err
    (add-face-text-property "1" 2 'bold)
  (wrong-type-argument (list 'add-face-text-property-beg (car err) (cdr err))))
(condition-case err
    (add-face-text-property 1 2 'bold nil t)
  (wrong-type-argument (list 'add-face-text-property-object (car err) (cdr err))))

(bidi-resolved-levels)
(bidi-resolved-levels nil)
(bidi-resolved-levels 1)
(condition-case err
    (bidi-resolved-levels t)
  (wrong-type-argument (list 'bidi-resolved-levels-type (car err) (cdr err))))

(bidi-find-overridden-directionality "abc" 0 "x")
(bidi-find-overridden-directionality nil 0 "x")
(bidi-find-overridden-directionality 1 2 nil)
(condition-case err
    (bidi-find-overridden-directionality 1 2 3)
  (wrong-type-argument (list 'bidi-find-overridden-directionality-third (car err) (cdr err))))
(condition-case err
    (bidi-find-overridden-directionality nil 2 nil)
  (wrong-type-argument (list 'bidi-find-overridden-directionality-region (car err) (cdr err))))

(> (color-distance "#000" "#fff") 0)
(= (color-distance "#000" "#000") 0)
(= (color-distance "#000" "#111") 0)
(condition-case err
    (color-distance "#00" "#fff")
  (error (list 'color-distance-invalid-left (car err) (cdr err))))
(condition-case err
    (color-distance 1 "#fff")
  (error (list 'color-distance-invalid-type (car err) (cdr err))))
(condition-case err
    (color-distance "#000" "#fff" t)
  (wrong-type-argument (list 'color-distance-frame (car err) (cdr err))))
