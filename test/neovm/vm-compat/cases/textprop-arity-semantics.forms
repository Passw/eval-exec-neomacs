; Lock in text-property builtin max-arity behavior.
(condition-case err (put-text-property 1 2 'face 'bold nil nil) (error err))
(condition-case err (get-text-property 1 'face nil nil) (error err))
(condition-case err (get-char-property 1 'face nil nil) (error err))
(condition-case err (add-text-properties 1 2 '(face bold) nil nil) (error err))
(condition-case err (remove-text-properties 1 2 '(face bold) nil nil) (error err))
(condition-case err (text-properties-at 1 nil nil) (error err))
(condition-case err (next-single-property-change 1 'face nil nil nil) (error err))
(condition-case err (previous-single-property-change 1 'face nil nil nil) (error err))
(condition-case err (next-property-change 1 nil nil nil) (error err))
(condition-case err (text-property-any 1 2 'face 'bold nil nil) (error err))

; Keep one non-error sanity path for put/get and text-property-any.
(with-temp-buffer
  (insert "ab")
  (put-text-property 1 2 'face 'bold)
  (get-text-property 1 'face))

(with-temp-buffer
  (insert "ab")
  (put-text-property 1 2 'face 'bold)
  (text-property-any 1 2 'face 'bold))
