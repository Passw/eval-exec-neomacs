(fboundp 'string-chop-newline)
(fboundp 'string-pad)
(fboundp 'string-fill)
(fboundp 'string-limit)

(let ((f (symbol-function 'string-pad))) (list (nth 1 f) (nth 3 f) (nth 4 f) (substring (nth 2 f) 0 20)))
(let ((f (symbol-function 'string-fill))) (list (nth 1 f) (nth 3 f) (nth 4 f) (substring (nth 2 f) 0 24)))
(let ((f (symbol-function 'string-limit))) (list (nth 1 f) (nth 3 f) (nth 4 f) (substring (nth 2 f) 0 26)))
(let ((f (symbol-function 'string-chop-newline))) (list (nth 1 f) (nth 3 f) (nth 4 f) (substring (nth 2 f) 0 24)))

(condition-case err (string-chop-newline (string 120 10)) (error err))
(condition-case err (string-pad "x" 2) (error err))
(condition-case err (string-fill "x" 2) (error err))
(condition-case err (string-limit "x" 2) (error err))
