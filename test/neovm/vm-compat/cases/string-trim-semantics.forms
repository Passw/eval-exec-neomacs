; string-trim/string-trim-left/string-trim-right optional regexp semantics
(string-trim "  a  ")
(string-trim-left "  a  ")
(string-trim-right "  a  ")
(string-trim "xxaYY" "x+" "Y+")
(string-trim-left "xxa" "x+")
(string-trim-right "aYY" "Y+")
(condition-case err (string-trim 1) (error (car err)))
(condition-case err (string-trim "a" nil nil nil) (error (car err)))
(condition-case err (string-trim-left "a" nil nil) (error (car err)))
(condition-case err (string-trim-right "a" nil nil) (error (car err)))
