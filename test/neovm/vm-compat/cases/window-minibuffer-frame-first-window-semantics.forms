(mapcar (lambda (s) (list s (fboundp s))) '(minibuffer-window window-minibuffer-p frame-first-window))
(list (subr-arity (symbol-function 'minibuffer-window)) (subr-arity (symbol-function 'window-minibuffer-p)) (subr-arity (symbol-function 'frame-first-window)))
(let ((m (minibuffer-window))
      (w (selected-window)))
  (list (windowp m)
        (window-live-p m)
        (window-minibuffer-p m)
        (window-minibuffer-p w)
        (window-minibuffer-p)
        (window-minibuffer-p nil)
        (eq (window-frame m) (selected-frame))
        (eq m (car (last (window-list nil t))))
        (eq (frame-first-window) w)
        (eq (frame-first-window w) w)
        (eq (frame-first-window m) w)
        (eq (minibuffer-window (selected-frame)) m)))
(list (condition-case err (minibuffer-window 999999) (error err))
      (condition-case err (window-minibuffer-p 999999) (error err))
      (condition-case err (frame-first-window 999999) (error err))
      (condition-case err (minibuffer-window (selected-window)) (error (car err)))
      (condition-case err (minibuffer-window nil nil) (error err))
      (condition-case err (window-minibuffer-p nil nil) (error err))
      (condition-case err (frame-first-window nil nil) (error err)))
