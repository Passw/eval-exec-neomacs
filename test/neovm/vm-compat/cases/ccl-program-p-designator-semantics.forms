; ccl-program-p should accept registered symbol designators.
(mapcar (lambda (s)
          (list s
                (fboundp s)
                (subrp (symbol-function s))
                (subr-arity (symbol-function s))
                (commandp s)))
        '(ccl-program-p register-ccl-program))

(ccl-program-p 'neovm-ccl-program-p-missing)
(register-ccl-program 'neovm-ccl-program-p-live [10 0 0])
(ccl-program-p 'neovm-ccl-program-p-live)
(ccl-program-p [10 0 0])
(ccl-program-p [1])
