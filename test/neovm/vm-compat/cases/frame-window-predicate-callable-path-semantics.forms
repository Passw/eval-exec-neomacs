(mapcar (lambda (s) (list s (fboundp s)))
        '(framep windowp))

(list (subr-arity (symbol-function 'framep))
      (subr-arity (symbol-function 'windowp)))

(let ((f (selected-frame))
      (w (selected-window)))
  (list (funcall #'framep f)
        (apply #'framep (list f))
        (funcall (indirect-function 'framep) f)
        (funcall #'framep w)
        (apply #'framep (list w))
        (funcall (indirect-function 'framep) w)
        (funcall #'windowp w)
        (apply #'windowp (list w))
        (funcall (indirect-function 'windowp) w)
        (funcall #'windowp f)
        (apply #'windowp (list f))
        (funcall (indirect-function 'windowp) f)))

(list (funcall #'framep nil)
      (apply #'framep '("x"))
      (funcall (indirect-function 'framep) 999999)
      (funcall #'windowp nil)
      (apply #'windowp '("x"))
      (funcall (indirect-function 'windowp) 999999))

(list (condition-case err (funcall #'framep) (error (car err)))
      (condition-case err (apply #'framep '()) (error (car err)))
      (condition-case err (funcall #'framep nil nil) (error (car err)))
      (condition-case err (apply #'framep '(nil nil)) (error (car err)))
      (condition-case err (funcall #'windowp) (error (car err)))
      (condition-case err (apply #'windowp '()) (error (car err)))
      (condition-case err (funcall #'windowp nil nil) (error (car err)))
      (condition-case err (apply #'windowp '(nil nil)) (error (car err))))
