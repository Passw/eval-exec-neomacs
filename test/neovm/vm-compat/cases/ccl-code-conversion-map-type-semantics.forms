; register-code-conversion-map type contract semantics.
(mapcar (lambda (s)
          (list s
                (fboundp s)
                (subrp (symbol-function s))
                (subr-arity (symbol-function s))
                (commandp s)))
        '(register-code-conversion-map))

(condition-case err
    (register-code-conversion-map 'neovm-map nil)
  (error err))

(condition-case err
    (register-code-conversion-map 'neovm-map "x")
  (error err))

(condition-case err
    (register-code-conversion-map 'neovm-map 1)
  (error err))

(condition-case err
    (register-code-conversion-map 'neovm-map 'foo)
  (error err))

(register-code-conversion-map 'neovm-map [1 2 3])
(register-code-conversion-map 'neovm-map [4 5 6])
