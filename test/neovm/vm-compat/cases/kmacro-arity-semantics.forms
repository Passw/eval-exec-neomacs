; Lock in keyboard macro builtin arity signaling.
(fboundp 'start-kbd-macro)
(condition-case err (start-kbd-macro nil nil) (error (car err)))

(fboundp 'end-kbd-macro)
(condition-case err (end-kbd-macro nil nil nil) (error (car err)))

(fboundp 'call-last-kbd-macro)
(condition-case err (call-last-kbd-macro nil nil nil) (error (car err)))

(fboundp 'execute-kbd-macro)
(condition-case err (execute-kbd-macro) (error (car err)))
(condition-case err (execute-kbd-macro nil nil nil nil) (error (car err)))

(fboundp 'insert-kbd-macro)
(condition-case err (insert-kbd-macro) (error (car err)))
(condition-case err (insert-kbd-macro 'x nil nil) (error (car err)))
