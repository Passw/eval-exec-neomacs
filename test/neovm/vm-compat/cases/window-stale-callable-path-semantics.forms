(mapcar (lambda (s) (list s (fboundp s)))
        '(window-buffer window-point window-frame select-window set-window-buffer))

(list (subr-arity (symbol-function 'window-buffer))
      (subr-arity (symbol-function 'window-point))
      (subr-arity (symbol-function 'window-frame))
      (subr-arity (symbol-function 'select-window))
      (subr-arity (symbol-function 'set-window-buffer)))

(let ((w (split-window)))
  (delete-window w)
  (list (condition-case err (window-buffer w) (error (car err)))
        (condition-case err (funcall #'window-buffer w) (error (car err)))
        (condition-case err (apply #'window-buffer (list w)) (error (car err)))
        (condition-case err (funcall (indirect-function 'window-buffer) w) (error (car err)))))

(let ((w (split-window)))
  (delete-window w)
  (list (condition-case err (window-point w) (error (car err)))
        (condition-case err (funcall #'window-point w) (error (car err)))
        (condition-case err (apply #'window-point (list w)) (error (car err)))
        (condition-case err (funcall (indirect-function 'window-point) w) (error (car err)))))

(let ((w (split-window)))
  (delete-window w)
  (list (condition-case err (window-frame w) (error (car err)))
        (condition-case err (funcall #'window-frame w) (error (car err)))
        (condition-case err (apply #'window-frame (list w)) (error (car err)))
        (condition-case err (funcall (indirect-function 'window-frame) w) (error (car err)))))

(let ((w (split-window)))
  (delete-window w)
  (list (condition-case err (select-window w) (error (car err)))
        (condition-case err (funcall #'select-window w) (error (car err)))
        (condition-case err (apply #'select-window (list w)) (error (car err)))
        (condition-case err (funcall (indirect-function 'select-window) w) (error (car err)))))

(let ((w (split-window)))
  (delete-window w)
  (list (condition-case err (set-window-buffer w (current-buffer)) (error (car err)))
        (condition-case err (funcall #'set-window-buffer w (current-buffer)) (error (car err)))
        (condition-case err (apply #'set-window-buffer (list w (current-buffer))) (error (car err)))
        (condition-case err (funcall (indirect-function 'set-window-buffer) w (current-buffer)) (error (car err)))))
