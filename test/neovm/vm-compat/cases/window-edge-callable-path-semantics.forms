(let ((w (selected-window)))
  (list (integerp (funcall #'window-mode-line-height w))
        (integerp (funcall #'window-header-line-height w))
        (integerp (funcall #'window-pixel-height w))
        (integerp (funcall #'window-pixel-width w))
        (integerp (funcall #'window-text-height w))
        (integerp (funcall #'window-text-width w))
        (integerp (funcall #'window-text-height w t))
        (integerp (funcall #'window-text-width w t))))

(let ((w (selected-window)))
  (list (integerp (apply #'window-mode-line-height (list w)))
        (integerp (apply #'window-header-line-height (list w)))
        (integerp (apply #'window-pixel-height (list w)))
        (integerp (apply #'window-pixel-width (list w)))
        (integerp (apply #'window-text-height (list w t)))
        (integerp (apply #'window-text-width (list w t)))))

(let ((w (selected-window)))
  (list (= (length (funcall #'window-body-pixel-edges w)) 4)
        (= (length (funcall #'window-pixel-edges w)) 4)
        (= (length (funcall #'window-body-edges w)) 4)
        (= (length (funcall #'window-edges w)) 4)
        (= (length (funcall #'window-inside-pixel-edges w)) 4)
        (= (length (funcall #'window-inside-edges w)) 4)))

(let ((w (selected-window)))
  (list (= (length (apply #'window-body-pixel-edges (list w))) 4)
        (= (length (apply #'window-pixel-edges (list w))) 4)
        (= (length (apply #'window-body-edges (list w))) 4)
        (= (length (apply #'window-edges (list w nil nil nil))) 4)
        (= (length (apply #'window-inside-pixel-edges (list w))) 4)
        (= (length (apply #'window-inside-edges (list w))) 4)))

(let ((fn (indirect-function 'window-edges))
      (w (selected-window)))
  (= (length (funcall fn w)) 4))
(let ((fn (indirect-function 'window-inside-edges))
      (w (selected-window)))
  (= (length (funcall fn w)) 4))

(condition-case err (funcall #'window-mode-line-height nil nil) (error err))
(condition-case err (funcall #'window-header-line-height nil nil) (error err))
(condition-case err (funcall #'window-pixel-height nil nil) (error err))
(condition-case err (funcall #'window-pixel-width nil nil) (error err))
(condition-case err (funcall #'window-text-height nil nil nil) (error err))
(condition-case err (funcall #'window-text-width nil nil nil) (error err))
(condition-case err (funcall #'window-body-pixel-edges nil nil) (error err))
(condition-case err (funcall #'window-pixel-edges nil nil) (error err))
(condition-case err (funcall #'window-body-edges nil nil) (error err))
(condition-case err (funcall #'window-inside-pixel-edges nil nil) (error err))
(condition-case err (funcall #'window-inside-edges nil nil) (error err))
(condition-case err (funcall #'window-edges nil nil nil nil nil) (error err))
