; CCL header slot-2 must be integer in range [0..3].
(mapcar (lambda (s)
          (list s
                (fboundp s)
                (subrp (symbol-function s))
                (subr-arity (symbol-function s))
                (commandp s)))
        '(ccl-program-p register-ccl-program))

(ccl-program-p [10 0 0])
(ccl-program-p [10 3 0])
(ccl-program-p [10 4 0])
(ccl-program-p [10 -1 0])
(ccl-program-p [10 97 0])
(ccl-program-p [9 4 0])
(ccl-program-p [1 2 3])
(ccl-program-p [1 4 3])

(condition-case err (register-ccl-program 'hdr-ok-a [10 0 0]) (error err))
(condition-case err (register-ccl-program 'hdr-ok-b [10 3 0]) (error err))
(condition-case err (register-ccl-program 'hdr-bad-a [10 4 0]) (error err))
(condition-case err (register-ccl-program 'hdr-bad-b [10 -1 0]) (error err))
(condition-case err (register-ccl-program 'hdr-bad-c [10 97 0]) (error err))
