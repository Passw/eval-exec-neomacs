(mapcar (lambda (s) (list s (fboundp s)))
        '(window-dedicated-p set-window-dedicated-p))

(list (subr-arity (symbol-function 'window-dedicated-p))
      (subr-arity (symbol-function 'set-window-dedicated-p)))

(let ((w (selected-window)))
  (list (window-dedicated-p w)
        (funcall #'window-dedicated-p w)
        (apply #'window-dedicated-p (list w))
        (let ((fn (indirect-function 'window-dedicated-p)))
          (funcall fn w))))

(let ((w (split-window)))
  (unwind-protect
      (list (window-dedicated-p w)
            (progn (funcall #'set-window-dedicated-p w t)
                   (window-dedicated-p w))
            (progn (apply #'set-window-dedicated-p (list w nil))
                   (window-dedicated-p w))
            (progn (let ((fn (indirect-function 'set-window-dedicated-p)))
                     (funcall fn w t))
                   (window-dedicated-p w))
            (progn (let ((fn (indirect-function 'set-window-dedicated-p)))
                     (funcall fn w nil))
                   (window-dedicated-p w)))
    (when (window-live-p w)
      (delete-window w))))

(list (condition-case err (funcall #'window-dedicated-p 999999) (error (car err)))
      (condition-case err (apply #'window-dedicated-p '(999999)) (error (car err)))
      (condition-case err (funcall #'set-window-dedicated-p 999999 t) (error (car err)))
      (condition-case err (apply #'set-window-dedicated-p '(999999 t)) (error (car err)))
      (condition-case err (funcall #'set-window-dedicated-p 'foo t) (error (car err)))
      (condition-case err (apply #'set-window-dedicated-p '(foo t)) (error (car err))))

(list (condition-case err (funcall #'window-dedicated-p nil nil) (error (car err)))
      (condition-case err (apply #'window-dedicated-p '(nil nil)) (error (car err)))
      (condition-case err (funcall #'set-window-dedicated-p nil) (error (car err)))
      (condition-case err (apply #'set-window-dedicated-p '(nil)) (error (car err)))
      (condition-case err (funcall #'set-window-dedicated-p nil t nil) (error (car err)))
      (condition-case err (apply #'set-window-dedicated-p '(nil t nil)) (error (car err))))
