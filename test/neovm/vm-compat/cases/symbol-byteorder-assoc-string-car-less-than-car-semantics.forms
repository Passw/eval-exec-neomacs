(fboundp 'bare-symbol)
(subrp (symbol-function 'bare-symbol))
(subr-arity (symbol-function 'bare-symbol))

(fboundp 'bare-symbol-p)
(subrp (symbol-function 'bare-symbol-p))
(subr-arity (symbol-function 'bare-symbol-p))

(fboundp 'byteorder)
(subrp (symbol-function 'byteorder))
(subr-arity (symbol-function 'byteorder))

(fboundp 'assoc-string)
(subrp (symbol-function 'assoc-string))
(subr-arity (symbol-function 'assoc-string))

(fboundp 'car-less-than-car)
(subrp (symbol-function 'car-less-than-car))
(subr-arity (symbol-function 'car-less-than-car))

(list 'bare-symbol
      (bare-symbol 'alpha)
      (bare-symbol :k)
      (bare-symbol nil)
      (bare-symbol-p 'alpha)
      (bare-symbol-p :k)
      (bare-symbol-p nil)
      (bare-symbol-p 1))

(condition-case err
    (bare-symbol 1)
  (wrong-type-argument (list 'bare-symbol-type (car err) (cdr err))))

(condition-case err
    (bare-symbol)
  (wrong-number-of-arguments (car err)))

(condition-case err
    (bare-symbol-p nil nil)
  (wrong-number-of-arguments (car err)))

(byteorder)

(condition-case err
    (byteorder nil)
  (wrong-number-of-arguments (car err)))

(list 'assoc-string
      (assoc-string "a" '(("a" . 1) ("b" . 2)))
      (assoc-string "A" '(("a" . 1) ("b" . 2)))
      (assoc-string "A" '(("a" . 1) ("b" . 2)) t)
      (assoc-string 'foo '((foo . 1) (:k . 2)))
      (assoc-string :k '((":k" . 1) (:k . 2)))
      (assoc-string "foo" '(1 ("bar" . 0) (foo . 9) ("foo" . 10)))
      (assoc-string "x" 1))

(condition-case err
    (assoc-string 1 '(("1" . 1)))
  (wrong-type-argument (list 'assoc-string-type (car err) (cdr err))))

(condition-case err
    (assoc-string "a" '(("a" . 1)) nil nil)
  (wrong-number-of-arguments (car err)))

(list 'car-less-than-car
      (car-less-than-car '(1 . a) '(2 . b))
      (car-less-than-car '(2 . a) '(2 . b))
      (car-less-than-car '(1.5 . a) '(2 . b)))

(condition-case err
    (car-less-than-car 1 '(2 . b))
  (wrong-type-argument (list 'car-less-left-type (car err) (cdr err))))

(condition-case err
    (car-less-than-car '(1 . a) 2)
  (wrong-type-argument (list 'car-less-right-type (car err) (cdr err))))

(condition-case err
    (car-less-than-car '(a . b) '(2 . b))
  (wrong-type-argument (list 'car-less-number-type (car err) (cdr err))))
