(let ((ht (make-hash-table :test 'eql :size 5)))
  (puthash -0.0 'neg ht)
  (puthash 0.0 'pos ht)
  (puthash 0.0e+NaN 'nan ht)
  (list (hash-table-count ht)
        (sort (mapcar #'cdr (apply #'append (internal--hash-table-buckets ht))) #'<)
        (gethash -0.0 ht 'miss)
        (gethash 0.0 ht 'miss)
        (gethash 0.0e+NaN ht 'miss)))

(let ((ht (make-hash-table :test 'equal :size 5)))
