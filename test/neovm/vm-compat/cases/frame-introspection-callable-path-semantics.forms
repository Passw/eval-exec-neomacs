(mapcar (lambda (s) (list s (fboundp s)))
        '(frame-visible-p frame-parameter frame-parameters
          modify-frame-parameters select-frame-set-input-focus))

(list (subr-arity (symbol-function 'frame-visible-p))
      (subr-arity (symbol-function 'frame-parameter))
      (subr-arity (symbol-function 'frame-parameters))
      (subr-arity (symbol-function 'modify-frame-parameters))
      (subr-arity (symbol-function 'select-frame-set-input-focus)))

(let ((f (selected-frame)))
  (list (not (null (funcall #'frame-visible-p f)))
        (not (null (apply #'frame-visible-p (list f))))
        (let ((fn (indirect-function 'frame-visible-p)))
          (not (null (funcall fn f))))
        (stringp (funcall #'frame-parameter f 'name))
        (stringp (apply #'frame-parameter (list f 'name)))
        (let ((fn (indirect-function 'frame-parameter)))
          (stringp (funcall fn f 'name)))
        (listp (funcall #'frame-parameters f))
        (listp (apply #'frame-parameters (list f)))
        (let ((fn (indirect-function 'frame-parameters)))
          (listp (funcall fn f)))))

(let ((f (selected-frame)))
  (list (condition-case err
            (progn (funcall #'modify-frame-parameters f nil) 'ok)
          (error (car err)))
        (condition-case err
            (progn (apply #'modify-frame-parameters (list f nil)) 'ok)
          (error (car err)))
        (condition-case err
            (progn (funcall (indirect-function 'modify-frame-parameters) f nil) 'ok)
          (error (car err)))
        (stringp (frame-parameter f 'name))))

(save-current-buffer
  (let ((f (selected-frame)))
    (list (framep (funcall #'select-frame-set-input-focus f))
          (framep (apply #'select-frame-set-input-focus (list f)))
          (framep (funcall (indirect-function 'select-frame-set-input-focus) f))
          (eq (selected-frame) f))))

(list (condition-case err (funcall #'frame-visible-p 999999) (error (car err)))
      (condition-case err (apply #'frame-visible-p '(999999)) (error (car err)))
      (condition-case err (funcall #'frame-visible-p nil nil) (error (car err)))
      (condition-case err (apply #'frame-visible-p '(nil nil)) (error (car err)))
      (condition-case err (funcall #'frame-parameter 999999 'name) (error (car err)))
      (condition-case err (apply #'frame-parameter '(999999 name)) (error (car err)))
      (condition-case err (funcall #'frame-parameter nil 'name nil) (error (car err)))
      (condition-case err (apply #'frame-parameter '(nil name nil)) (error (car err)))
      (condition-case err (funcall #'frame-parameters 999999) (error (car err)))
      (condition-case err (apply #'frame-parameters '(999999)) (error (car err)))
      (condition-case err (funcall #'frame-parameters nil nil) (error (car err)))
      (condition-case err (apply #'frame-parameters '(nil nil)) (error (car err)))
      (condition-case err (funcall #'modify-frame-parameters 999999 nil) (error (car err)))
      (condition-case err (apply #'modify-frame-parameters '(999999 nil)) (error (car err)))
      (condition-case err (funcall #'modify-frame-parameters nil nil nil) (error (car err)))
      (condition-case err (apply #'modify-frame-parameters '(nil nil nil)) (error (car err)))
      (condition-case err (funcall #'select-frame-set-input-focus 999999) (error (car err)))
      (condition-case err (apply #'select-frame-set-input-focus '(999999)) (error (car err)))
      (condition-case err (funcall #'select-frame-set-input-focus nil nil) (error (car err)))
      (condition-case err (apply #'select-frame-set-input-focus '(nil nil)) (error (car err))))
