; indent-to should return the reached column, not the requested target.
(list
 (with-temp-buffer
   (insert "abcdef")
   (goto-char (point-max))
   (list (current-column)
         (indent-to 2)
         (current-column)))
 (with-temp-buffer
   (list (current-column)
         (indent-to 4)
         (current-column)))
 (with-temp-buffer
   (list (current-column)
         (indent-to 2 5)
         (current-column)))
 (with-temp-buffer
   (insert "ab")
   (goto-char (point-max))
   (list (current-column)
         (indent-to 6 1)
         (current-column))))
