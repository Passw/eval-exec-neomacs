(fboundp 'byte-to-position)
(subrp (symbol-function 'byte-to-position))
(subr-arity (symbol-function 'byte-to-position))

(fboundp 'byte-to-string)
(subrp (symbol-function 'byte-to-string))
(subr-arity (symbol-function 'byte-to-string))

(fboundp 'bitmap-spec-p)
(subrp (symbol-function 'bitmap-spec-p))
(subr-arity (symbol-function 'bitmap-spec-p))

(fboundp 'clear-face-cache)
(subrp (symbol-function 'clear-face-cache))
(subr-arity (symbol-function 'clear-face-cache))

(fboundp 'clear-buffer-auto-save-failure)
(subrp (symbol-function 'clear-buffer-auto-save-failure))
(subr-arity (symbol-function 'clear-buffer-auto-save-failure))

(with-temp-buffer
  (list 'byte-to-position-empty
        (byte-to-position 1)
        (byte-to-position 2)
        (byte-to-position 0)
        (byte-to-position -1)))

(with-temp-buffer
  (insert "a√©")
  (list 'byte-to-position-filled
        (byte-to-position 1)
        (byte-to-position 2)
        (byte-to-position 3)
        (byte-to-position 4)
        (byte-to-position 5)))

(condition-case err
    (byte-to-position "x")
  (wrong-type-argument (list 'byte-to-position-type (car err) (cdr err))))
(condition-case err
    (byte-to-position 1 2)
  (wrong-number-of-arguments (car err)))

(list 'byte-to-string-bytes
      (get-byte 0 (byte-to-string 0))
      (get-byte 0 (byte-to-string 65))
      (get-byte 0 (byte-to-string 255)))
(condition-case err
    (byte-to-string 'x)
  (wrong-type-argument (list 'byte-to-string-type (car err) (cdr err))))
(condition-case err
    (byte-to-string 256)
  (error (list 'byte-to-string-range (car err) (cadr err))))

(list 'bitmap-spec-p
      (bitmap-spec-p nil)
      (bitmap-spec-p '(1 2 3))
      (bitmap-spec-p [1 2]))
(condition-case err
    (bitmap-spec-p)
  (wrong-number-of-arguments (car err)))

(list 'clear-face-cache
      (clear-face-cache)
      (clear-face-cache nil)
      (clear-face-cache 'all))
(condition-case err
    (clear-face-cache nil nil)
  (wrong-number-of-arguments (car err)))

(clear-buffer-auto-save-failure)
(condition-case err
    (clear-buffer-auto-save-failure nil)
  (wrong-number-of-arguments (car err)))
